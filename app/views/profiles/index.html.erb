<p style="color: green">
  <%= notice %>
</p>

<h1 class="text-muted text-center">
  Profiles
</h1>

<% @profiles.each do |profile| %>
  <div class="card shadow p-5 my-5">
    <%= render profile %>
    <p><%= link_to "Show this profile", profile, class: "btn btn-sm btn-outline-primary mt-3" %></p>
  </div>
<% end %>

<%= link_to "New profile", new_profile_path, class: "btn btn-sm btn-outline-primary" %>
